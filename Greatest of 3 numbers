#include<stdio.h>
main()
{
    int n1,n2,n3;
    printf("Enter the values of 3 numbers\n");
    scanf("%d",&n1);
    scanf("%d",&n2);
    scanf("%d",&n3);
    if(n1>=n2 && n1>=n3)
        printf("%d is greater number",n1);
    else if(n2>=n3 && n2>=n1)
        printf("%d is greater number",n2);
    else if(n3>=n2 && n3>=n1)
        printf("%d is greater number",n3);
}
